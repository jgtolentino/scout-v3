# Database Provider Configuration
# Choose one: supabase, azure-sql, azure-postgresql
VITE_DB_PROVIDER=supabase

# Supabase Configuration (if using Supabase)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Azure Database Configuration (if using Azure databases)
VITE_AZURE_DB_CONNECTION_STRING=your_azure_connection_string
VITE_AZURE_DB_SERVER=your_azure_server.database.windows.net
VITE_AZURE_DB_DATABASE=ScoutAnalyticsV3
VITE_AZURE_DB_USERNAME=scoutadmin
VITE_AZURE_DB_PASSWORD=your_secure_password

# Azure OpenAI Configuration (for AI Insights)
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com
AZURE_OPENAI_KEY=your_azure_openai_api_key
AZURE_OPENAI_DEPLOYMENT=gpt-4o
AZURE_OPENAI_API_VERSION=2024-05-01-preview

# Optional: Development flags
NODE_ENV=development
VITE_APP_ENV=development
VITE_SCOUT_DEMO=off